@model List<YC.Model.Group>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table>
    <tr>
        <th>Name</th>
        <th>UnitName</th>
        <th>Semester</th>
    </tr>

    @for (var i = 0; i < Model.Count; i++)
    {
        <tr>
            <td>@(Model[i].Name)</td>
            <td>@(Model[i].UnitName)</td>
            <td>@(Model[i].Semester)</td>
        </tr>

    }
</table>



